    "import pandas as pd\n",
    "\n",
    "# Step 1: Original data\n",
    "data = {\n",
    "    \"Channel\": [\"Facebook Ads\", \"Email Campaign\", \"LinkedIn DMs\"],\n",
    "    \"Leads\": [3000, 1000, 500],\n",
    "    \"Cost\": [90000, 10000, 25000],\n",
    "    \"Conversions\": [30, 25, 10]\n",
    "}\n",
    "df = pd.DataFrame(data)\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 9,
   "id": "71b68d34-e21c-41cc-abf4-1b54f79a7d43",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
