    "# Funnel of Sales last 4-6 Weeks"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "id": "5cc882b3-30d3-44f8-b527-7dec89d51349",
   "metadata": {},
   "outputs": [],
   "source": [
    "import matplotlib.pyplot as plt"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "d826954f-df25-4245-887e-9e49aaccfbc2",
   "metadata": {},
   "outputs": [],
   "source": [
    "stages = [\"Lead\", \"MQL\", \"PQL\", \"SQL\", \"CS\"]\n",
    "widths = [5000, 2500, 1200, 600, 300]   # counts for different diffrent stages\n",
    "owners = [\"Marketing\", \"Marketing\", \"Product + Sales\", \"Sales\", \"Sales â†’ CS\"]\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "c8acb88e-604a-4c3d-9821-64c8808f67d4",
   "metadata": {},
   "outputs": [
    {
     "data": {
      "image/png": "",
      "text/plain": [
       "<Figure size 800x600 with 1 Axes>"
      ]
     },
     "metadata": {},
     "output_type": "display_data"
    }
   ],
   "source": [
    "fig, ax = plt.subplots(figsize=(8,6))\n",
    "\n",
    "for i, (stage, width, owner) in enumerate(zip(stages, widths, owners)):\n",
    "# here is funnel chart \n",
    "    ax.barh(i, width, color=\"skyblue\", edgecolor=\"black\")\n",
    "    \n",
    "# add stage name and counts also \n",
    "    ax.text(width/2, i, f\"{stage}\\n({width})\", \n",
    "            va=\"center\", ha=\"center\", fontsize=11, weight=\"bold\", color=\"black\")\n",
    "    \n",
    "# Add owner on the right side\n",
    "    ax.text(width + 15, i, f\"Owner: {owner}\", \n",
    "            va=\"center\", ha=\"left\", fontsize=10, color=\"darkblue\")\n",
    "\n",
    "ax.invert_yaxis() #for top to bottom\n",
    "ax.axis(\"off\")\n",
    "plt.title(\"B2B SaaS Sales Funnel with Counts & Ownership\", fontsize=14, weight=\"bold\")\n",
    "plt.show()"
   ]
  },
